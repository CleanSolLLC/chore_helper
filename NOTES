Chore Helper Management System

Models
	User
		has_many :chores
		has_many :tasks, through: :chores

		:name => "Jim"
		:name => "Sue"
		:name => "Max"

		:email => "jim@foo.com"
		:email => "sue@bar.c0m"
		:email => "Maxgh@foobar.com"

		:pasword_digest => "Jim123abc"
		:pasword_digest => "Sue456def"
		:pasword_digest => "Max789ghi"


	Chore
		belongs_to :user
		belongs_to :task
		has_many :tasks

		:name => "cleaning"
		:name => "grocery shopping"
		:name => "laundry"


	Task
		belongs_to :chore
		has_many :users, through: chores
		:cleaning{ 
			[remove trash,
			clean kitchen,
			clean bathroom 1,
			clean bathroom 2,
			clean bedroom 1,
			Clean bedroom 2,
			]
		}

		:grocery shopping{
			[apples,
			eggs,
			milk,
			bread,
			chicken,
			fish,
			pet food
			]
		}

		:laundry{
			[separate whites From colors,
			wash clothes,
			dry clothes,
			fold and put away clothes
			]
		}

		:pet care{
			[walk Fido,
			change Slim Jim's litter box
			]
		}
